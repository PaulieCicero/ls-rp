ReturnLocationEx(playerid)
{
	new
		playerLocation[MAX_ZONE_NAME],
		playerCity[MAX_ZONE_NAME],
		playerStreet[MAX_ZONE_NAME],
		longStr[128]
	;

	GetPlayerStreet(playerid, playerStreet, MAX_ZONE_NAME);
	GetPlayer2DZone(playerid, playerLocation, MAX_ZONE_NAME);
	GetPlayerCity(playerid, playerCity, MAX_ZONE_NAME);

	format(longStr, sizeof(longStr), "%s, %s, %s %d", playerStreet, playerLocation, playerCity, ReturnAreaCode(playerLocation));
	return longStr;
}

ReturnLocation(playerid)
{
	new
		playerStreet[MAX_ZONE_NAME],
		playerLocation[MAX_ZONE_NAME],
		longStr[128]
	;

	GetPlayerStreet(playerid, playerStreet, MAX_ZONE_NAME);
	GetPlayer2DZone(playerid, playerLocation, MAX_ZONE_NAME);

	format(longStr, sizeof(longStr), "%s, %s", playerStreet, playerLocation);
	return longStr;
}

ReturnLocationPos(Float:x, Float:y)
{
	new string[60];
	format(string, 60, "Unknown");

 	for(new i = 0; i != sizeof(gSAZones); ++i)
 	{
		if(x >= gSAZones[i][SAZONE_AREA][0] && x <= gSAZones[i][SAZONE_AREA][3] && y >= gSAZones[i][SAZONE_AREA][1] && y <= gSAZones[i][SAZONE_AREA][4])
		{
		    format(string, 60, "%s", gSAZones[i][SAZONE_NAME]);
		    break;
		}
	}

	return string;
}

